    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "socket.io": "^3.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
